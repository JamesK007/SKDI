<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
	
	


		<auto_play>
			<defaults symbol="circular" time="3" />
			<tweenIn x="500" y="50" width="35" height="35" tint="0xFFFFFF" />
		</auto_play>

		
    	<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</prev_button>
		
    	<prev_symbol>
			<tweenOver tint="0x000000" />			
		</prev_symbol>
		
    	<next_button>
			<defaults round_corners="5,5,5,5"/>			
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</next_button>
		
    	<next_symbol>
			<tweenOver tint="0x000000" />
		</next_symbol>	
			
	</settings>    

	<slides>
	
        <slide>
            <url>imgs/header-img1.jpg</url>
        </slide>
		<!-- changing transition between first & second slide -->
        <transition num="1" slicing="vertical" direction="down"/>
		
        <slide>
       		<url>imgs/header-img2.jpg</url>
        </slide>		
		<transition num="2" slicing="vertical" direction="up" shader="flat" delay="0.05" z_multiplier="4" />
	
  		<slide>
       		<url>imgs/header-img3.jpg</url>
        </slide>   
		 <transition num="3" slicing="vertical" direction="down"/>
	 
  		<slide>
       		<url>imgs/header-img4.jpg</url>
        </slide>	
    	<transition num="4" slicing="vertical" direction="up" shader="flat" delay="0.05" z_multiplier="4" />
	
  		<slide>
       		<url>imgs/header-img5.jpg</url>
        </slide>
		
		<slide>
       		<url>imgs/golden-bug.jpg</url>
        </slide>
		
			
	</slides>
</cu3er>

